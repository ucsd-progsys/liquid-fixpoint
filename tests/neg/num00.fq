// This qualifier saves the day; solve constraints WITHOUT IT

qualif Zog(v:a) : (0 <= v)

bind 0 zog : {v : int | true}

constraint:
  env [0]
  grd true
  lhs {v : alpha | (v = 10)}
  rhs {v : alpha | $k0}
  id 1 

constraint:
  env [0]
  grd true
  lhs {v : alpha | $k0}
  rhs {v : alpha | $k0}
  id 2 

constraint:
  env [0]
  grd true 
  lhs {v : alpha | $k0}
  rhs {v : alpha | 0 <= v}
  id 3 

wf:
  env [0]
  reft {v: alpha | $k0}

